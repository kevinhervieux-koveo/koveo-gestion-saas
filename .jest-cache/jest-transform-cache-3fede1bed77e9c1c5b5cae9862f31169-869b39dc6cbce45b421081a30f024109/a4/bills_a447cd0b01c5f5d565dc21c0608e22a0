    legacyHeaders: false, // Disable the `X-RateLimit-*` headers
    keyGenerator: (req) => {
        // Rate limit per authenticated user ID (preferred) or use IP as fallback with IPv6 support
        return req.user?.id || (0, express_rate_limit_1.ipKeyGenerator)(req);
