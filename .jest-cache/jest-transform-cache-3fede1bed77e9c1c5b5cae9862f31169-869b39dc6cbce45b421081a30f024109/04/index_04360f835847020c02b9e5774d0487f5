                console.log("\u274C [BILLS UPLOAD] No file provided in request");
                return res.status(400).json({ message: "No file uploaded" });
            }
            console.log(`\u{1F4C4} [BILLS UPLOAD] File received:`, {
                originalName: req.file.originalname,
