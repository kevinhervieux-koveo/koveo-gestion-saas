            (cov_26jl6hyka6().b[5][0]++, user.email.charAt(0).toUpperCase()) :
            /* istanbul ignore next */
            (cov_26jl6hyka6().b[5][1]++, (0, jsx_runtime_1.jsx)(lucide_react_1.User, {
              className: 'w-5 h-5'
            })))
          })
        })) :
        /* istanbul ignore next */
        (/* Login Button - when not authenticated */cov_26jl6hyka6().b[3][1]++, (0, jsx_runtime_1.jsxs)(button_1.Button, {
          variant: 'ghost',
          size: 'sm',
          onClick: () => {
            /* istanbul ignore next */
            cov_26jl6hyka6().f[4]++;
            cov_26jl6hyka6().s[19]++;
            return setLocation('/login');
          },
          className: 'hidden sm:flex items-center gap-2 text-gray-600 hover:text-gray-800 h-10',
          "data-testid": 'header-login-button',
          children: [(0, jsx_runtime_1.jsx)(lucide_react_1.LogIn, {
            className: 'w-4 h-4'
          }), language === 'fr' ?
          /* istanbul ignore next */
          (cov_26jl6hyka6().b[6][0]++, 'Connexion') :
          /* istanbul ignore next */
          (cov_26jl6hyka6().b[6][1]++, 'Login')]
        }))]
      })]
    })]
  });
}
