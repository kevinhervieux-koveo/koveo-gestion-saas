            // Get the bill to check if it has a document
            const bill = await db_1.db.select().from(bills).where((0, drizzle_orm_1.eq)(bills.id, id)).limit(1);
            if (bill.length === 0) {
                console.error('âŒ [SERVER DEBUG] Bill not found for document download:', id);
