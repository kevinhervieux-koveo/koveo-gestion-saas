  "scripts": {
    "dev": "NODE_ENV=development npx tsx server/index.ts",
    "dev:client": "npx vite dev --host",
    "dev:server": "NODE_ENV=development npx tsx watch server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npx vite build",
    "build:server": "npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "preview": "npx vite preview",
    "typecheck": "npx tsc --noEmit",
    "typecheck:watch": "npx tsc --noEmit --watch",
    "db:push": "npx drizzle-kit push",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio",
    "db:generate": "npx drizzle-kit generate"
  },
