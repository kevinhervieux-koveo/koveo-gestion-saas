{
  "name": "koveo-gestion",
  "version": "1.0.0",
  "description": "Professional property management platform for Quebec with bilingual support, tenant portals, and automated communications",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Koveo Gestion Team",
    "email": "support@koveogestion.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koveogestion/platform.git"
  },
  "keywords": [
    "property-management",
    "quebec",
    "real-estate",
    "tenant-portal",
    "maintenance",
    "billing",
    "saas",
    "typescript",
    "react",
    "express"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npx tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "vite build",
    "build:server": "npx tsx scripts/build-server.ts",
    "db:push": "npx drizzle-kit push",
    "start": "NODE_ENV=production PORT=${PORT:-5000} node dist/index.js",
    "test": "npx jest --passWithNoTests=false --verbose",
    "test:watch": "npx jest --watch --passWithNoTests",
    "test:coverage": "npx jest --coverage --passWithNoTests=false --coverageThreshold='{\"global\":{\"branches\":75,\"functions\":75,\"lines\":75,\"statements\":75}}'",
    "test:sequential": "bash scripts/run-tests-sequential.sh",
    "test:unit": "npx jest tests/unit/ --passWithNoTests=false --verbose",
    "test:integration": "npx jest tests/integration/ --passWithNoTests=false --verbose",
    "test:e2e": "npx jest tests/e2e/ --passWithNoTests=false",
    "test:security": "bash scripts/run-security-tests.sh",
    "test:features": "npx tsx scripts/test-feature-requests.ts",
    "test:auth": "npx tsx scripts/test-auth-security.ts",
    "test:db-sync": "npx tsx scripts/test-database-sync.ts",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:db-sync && npm run test:security",
    "test:strict": "npx jest --passWithNoTests=false --verbose --detectOpenHandles --forceExit",
    "test:db-integrity": "npx jest tests/integration/database-sync.test.ts tests/integration/document-functionality.test.ts --passWithNoTests=false",
    "db:status": "./db-status.sh",
    "db:quick-check": "npx tsx scripts/dev-db-check.ts", 
    "db:sync-test": "./sync-test.sh",
    "db:sync-fix": "./sync-test.sh fix",
    "db:sync-watch": "./sync-test.sh watch"
    "quality:check": "npx tsx scripts/quick-quality-check.ts",
    "quality:full": "npx tsx scripts/run-quality-check.ts",
    "quality:consolidation": "npx tsx scripts/run-consolidation-quality.ts",
    "validate:all": "npx tsx scripts/validation-suite.ts",
    "validate:routes": "npx tsx scripts/validate-routes.ts",
    "validate:deployment": "npx tsx scripts/deployment-validation.ts",
    "validate:organization": "npx tsx scripts/validate-organization.ts",
    "validate:language": "npx tsx scripts/validate-language.ts",
    "analyze:features": "npx tsx scripts/analyze-feature-coverage.ts",
    "analyze:redundancy": "npx tsx scripts/run-redundancy-analysis.ts",
    "analyze:consolidate": "npx tsx scripts/consolidate-redundancies.ts",
    "security:check": "npx tsx scripts/quebec-security-check.ts",
    "security:report": "npx tsx scripts/generate-security-report.ts",
    "lint": "npx eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "npx eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "npx eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "db:seed": "npx tsx scripts/create-comprehensive-demo.ts",
    "db:admin": "npx tsx scripts/create-admin-user.ts",
    "db:migrate-docs": "npx tsx scripts/migrate-documents.ts",
    "docs:validate": "npx tsx scripts/docs-validation.ts",
    "docs:jsdoc": "npx tsx scripts/apply-jsdoc-templates.ts",
    "build:complete": "npx tsx scripts/build.ts",
    "post-build": "bash scripts/post-build.sh",
    "demo:create": "npx tsx scripts/create-comprehensive-demo.ts",
    "deploy:hooks": "npx tsx scripts/deployment-hooks.ts"
  },
